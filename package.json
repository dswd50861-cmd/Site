{
  "name": "ops-manager-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": ["server", "frontend"],
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:frontend\"",
    "dev:server": "npm --prefix server run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "build": "npm --prefix server run build && npm --prefix frontend run build"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
